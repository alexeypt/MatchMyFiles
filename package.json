{
    "name": "match-my-files",
    "description": "Match My Files is a powerful tool for detecting and managing duplicate files and folders. Effortlessly compare folders, identify duplicate files using advanced file hashing and size matching, and streamline your file organization.",
    "author": "https://github.com/alexeypt",
    "license": "MIT",
    "repository": "https://github.com/alexeypt/MatchMyFiles",
    "keywords": [
        "duplicate",
        "files",
        "folders",
        "comparison",
        "hashing",
        "size",
        "matching",
        "organization"
    ],
    "version": "1.0.0",
    "private": false,
    "scripts": {
        "dev": "npx dotenv -e .env.local -- node server.mjs",
        "build": "npx dotenv -e .env.production -- next build",
        "start": "npx dotenv -e .env.production -- node server.mjs",
        "ts-check": "tsc --noEmit",
        "lint": "eslint .",
        "lint-fix": "eslint --fix .",
        "prisma:generate:dev": "npx dotenv -e .env.local -- prisma generate",
        "prisma:generate-sql:dev": "npx dotenv -e .env.local -- prisma generate --sql",
        "prisma:migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
        "prisma:generate:prod": "npx dotenv -e .env.production -- prisma generate",
        "prisma:generate-sql:prod": "npx dotenv -e .env.production -- prisma generate --sql",
        "prisma:migrate:prod": "npx dotenv -e .env.production -- prisma migrate deploy"
    },
    "dependencies": {
        "@heroui/react": "^2.8.7",
        "@prisma/adapter-pg": "^7.2.0",
        "@prisma/client": "^7.2.0",
        "classnames": "^2.5.1",
        "exif-reader": "^2.0.3",
        "formik": "^2.4.9",
        "framer-motion": "^12.23.26",
        "next": "^16.1.1",
        "prisma": "^7.2.0",
        "react": "^19.2.3",
        "react-complex-tree": "^2.6.1",
        "react-dom": "^19.2.3",
        "react-toastify": "^11.0.5",
        "sharp": "^0.34.5",
        "socket.io": "^4.8.3",
        "socket.io-client": "^4.8.3",
        "yup": "^1.7.1"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^5.6.1",
        "@tailwindcss/postcss": "^4.1.18",
        "@types/node": "^25",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9.39.2",
        "eslint-config-next": "^16.1.1",
        "eslint-plugin-newline-destructuring": "^1.2.2",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "postcss": "^8",
        "tailwindcss": "^4.1.18",
        "typescript": "^5"
    }
}